[server]
http_port = 3000
domain = localhost
root_url = %(protocol)s://%(domain)s:%(http_port)s/
router_logging = false

[database]
type = sqlite3
path = {{ grafana_data_dir }}/grafana.db

[paths]
data = {{ grafana_data_dir }}
logs = {{ grafana_logs_dir }}

[auth.anonymous]
enabled = true
org_name = Main Org.
org_role = Viewer

[analytics]
reporting_enabled = false
check_for_updates = false
